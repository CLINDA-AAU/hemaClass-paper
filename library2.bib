issn = {0028-0836},
journal = {Nature},
keywords = {Adult,B-Cell,B-Cell: diagnosis,B-Cell: genetics,B-Lymphocytes,B-Lymphocytes: pathology,Chronic,Cultured,Diffuse,Diffuse: diagnosis,Diffuse: genetics,Gene Expression Profiling,Humans,Large B-Cell,Leukemia,Lymphocytic,Lymphoma,Oligonucleotide Array Sequence Analysis,Phenotype,Tumor Cells},
